  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/json/*.json > cypress/reports/json/report_final.json",
    "generate-report": "marge cypress/reports/json/report_final.json -f report -o cypress/reports/mochareports",
    "delete:reportFolder": "rm -rf mochawesome-report/ && rm -rf videos/",
    "test:cli": "npm run delete:reportFolder && cypress run",
    "test:gui": "npm run delete:reportFolder && npx cypress open",
    "merge:reports": "mochawesome-merge --reportDir mochawesome-report/*.json > cypress-combined-report.json"
  },
